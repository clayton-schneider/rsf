{
  "extends": "astro/tsconfigs/base",
  "exclude": ["functions/**/*"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@imgs/*": ["src/imgs/*"],
      "@data/*": ["src/data/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
